<link rel="stylesheet" href="/css/genres_list.css">
<section class="container">
    <div class="genres">
        <%genres.map((genre)=>{%>
            <div class="genre">
                <a href="/genres/<%=genre.name%>" class="filler">
                    <span><%=genre.name%></span>
                </a>
            </div>
        <%})%>
    </div>
</section>